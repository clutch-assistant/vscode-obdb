{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "webpack-compile",
      "type": "shell",
      "command": "npm",
      "args": [
        "run",
        "compile"
      ],
      "problemMatcher": {
        "pattern": {
          "regexp": "^.*(?:ERROR|error|Error) TS\\d+:(.*)$",
          "file": 1,
          "location": 2,
          "message": 3
        }
      },
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
